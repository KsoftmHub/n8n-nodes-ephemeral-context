{
  "name": "@ksoftm/n8n-nodes-ephemeral-context",
  "version": "1.3.1",
  "description": "An n8n node for execution-scoped, in-memory ephemeral storage.",
  "keywords": [
    "n8n-community-node-package"
  ],
  "license": "MIT",
  "author": "Ksoftm",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/KsoftmHub/n8n-nodes-ephemeral-context.git"
  },
  "publishConfig": {
    "access": "public"
  },
  "main": "index.js",
  "scripts": {
    "build": "node bin/build.js",
    "dev": "npm run build -- --watch",
    "lint": "tslint -p tsconfig.json -c tslint.json",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "files": [
    "dist"
  ],
